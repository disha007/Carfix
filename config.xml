<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.itforcegroup.carfix" version="1.0.0">
  <name>Carfix</name>
  <description>Carfix Application.</description>
  <author href="http://www.itforcegroup.com" email="info@itforcegroup.com">
    It Force
  </author>

  <preference name="android-minSdkVersion"      value="9" />
  <preference name="SplashScreen" value="screen" />


  <gap:plugin name="org.apache.cordova.network-information"/>
  <gap:plugin name="org.apache.cordova.splashscreen"/>

  
  <icon src="icon.png"/>
  <icon src="www/images/icon/android/icon-36-ldpi.png" gap:platform="android" gap:qualifier="ldpi"/>
  <icon src="www/images/icon/android/icon-48-mdpi.png" gap:platform="android" gap:qualifier="mdpi"/>
  <icon src="www/images/icon/android/icon-72-hdpi.png" gap:platform="android" gap:qualifier="hdpi"/>
  <icon src="www/images/icon/android/icon-96-xhdpi.png" gap:platform="android" gap:qualifier="xhdpi"/>
  
  <icon src="www/images/icon/ios/icon-57.png" gap:platform="ios" width="57" height="57"/>
  <icon src="www/images/icon/ios/icon-72.png" gap:platform="ios" width="72" height="72"/>
  <icon src="www/images/icon/ios/icon-57-2x.png" gap:platform="ios" width="114" height="114"/>
  <icon src="www/images/icon/ios/icon-72-2x.png" gap:platform="ios" width="144" height="144"/>


  <gap:splash src="splash.png" />

  

  <gap:config-file platform="ios" parent="CFBundleShortVersionString">
    <string>100</string>
  </gap:config-file>
  
  <access origin="*"/>
 
  
</widget>